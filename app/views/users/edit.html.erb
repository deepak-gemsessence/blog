<h3>Edit User Details</h3>

<% if logged_in? %>
  Welcome <%= current_user.first_name %>
<% end %> <br>

<table>
  <tr>
    <%= form_for @user do |f| %>
      <p>
        First name <%= f.text_field :first_name %>
      </p>
      <p>
        Last name<%= f.text_field :last_name %>
      </p>
      <p>
        Contact No<%= f.text_field :contact %>
      </p>
      <p>
        <%= f.submit %><br>
      </p>
    <% end %>
  </tr>
</table>

<%= render 'footer' %>