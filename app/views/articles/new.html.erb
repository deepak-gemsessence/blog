


<h1>new title</h1>

<%= render 'users/welcome' %> <br>

<%= form_for @article do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.submit class: "btn btn-success"  %><br>
  </p>
<% end %>
